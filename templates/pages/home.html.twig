{% extends 'base.html.twig' %}

{% block title %}Home{% endblock %}

{% block body %}

<div class="mt-3 mb-3">
    {% for type, messages in app.flashes %}
        {% for message in messages %}
            <div class="alert alert-{{ type }}">{{ message }}</div>
        {% endfor %}
    {% endfor %}
    </div>

<div class="contact"> 
    <div class="mb-3">
    <h3>{{ 'form.contact.title'|trans }}</h3>
    </div>
    {{ form(form) }}
 </div>

{% endblock %}